{
  "name": "Get Sourceid",
  "author": "@9-lives <911120823@qq.com.cn>",
  "version": "0.0.3",
  "manifest_version": 2,
  "minimum_chrome_version": "45",
  "description": "This chrome extension is used for getting screen's sourceId",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "icons": {
    "16": "./image/icon16.png",
    "48": "./image/icon48.png",
    "128": "./image/icon128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "./image/icon16.png",
    "./image/icon48.png",
    "./image/icon128.png"
  ]
}